{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug DiffInk",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/debug/bin/diffink",
      "args": [
        "~/Repos/diffink_benchmark/dataset/bcpp_cpp/data/original/19.cpp",
        "~/Repos/diffink_benchmark/dataset/bcpp_cpp/data/modified/19.cpp",
        "-l",
        "cpp",
        "--log",
        "-m",
        "gumtree-simple",
        "-f",
        "text",
        "json",
        "html"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "build"
    }
  ]
}
